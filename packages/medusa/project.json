{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/medusa/src",
    "projectType": "library",
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/medusa/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/packages/medusa"],
            "options": {
                "jestConfig": "packages/medusa/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
